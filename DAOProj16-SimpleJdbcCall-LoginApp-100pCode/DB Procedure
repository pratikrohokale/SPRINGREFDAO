CREATE OR REPLACE PROCEDURE P_AUTHENTICATION(U_NAME IN VARCHAR,PASS IN VARCHAR,RESULT OUT VARCHAR)AS
  CNT NUMBER;
BEGIN
  SELECT COUNT(*) INTO CNT FROM USERLIST WHERE UNAME=U_NAME AND PWD=PASS;
 IF(CNT<>0)THEN
    RESULT:='Valid Crendetials';
 ELSE 
    RESULT:='InValid Credentials';
 END IF;
END;
/
